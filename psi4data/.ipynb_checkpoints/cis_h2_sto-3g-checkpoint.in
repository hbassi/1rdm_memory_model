memory 1 gb

molecule h2 {
H  0.       0.      -0.37
H  0.       0.       0.37
symmetry c1
no_com
no_reorient
}

set {
    reference rhf
    basis sto-3g
    ms0 false
    ex_allow [1, 0]
    num_roots 3
    diag_method rsp
    print 2
    dipmom true
    opdm true
    tdm true
}

thisenergy, ciwfn = prop('cisd', properties=['DIPOLE', 'TRANSITION_DIPOLE'], return_wfn=True)

for i in range(3):
    for j in range(i+1):
        ciop_dm = ciwfn.get_opdm(i,j,'SUM',True)
        ciop_dm.print_out()

mints = MintsHelper(ciwfn.basisset())

AOdip = mints.ao_dipole()
AOdip[0].print_out()
AOdip[1].print_out()
AOdip[2].print_out()

ciwfn.Ca().print_out()               # Print orbital information


